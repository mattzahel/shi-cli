{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "never"],
        "quotes": [2, "single"],
        "no-console": 0,
        "quote-props": [2, "consistent"]
    }
}
